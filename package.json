{
	"name": "@ledge/ng-app",
	"version": "6.0.1",
	"description": "Powerful, easy-to-use app container for Angular.js with ES Modules. Types included. IE11 supported.",
	"homepage": "https://ng-app.js.org",
	"author": "Zack Schuster <email@zackschuster.com> (http://so.completely.online)",
	"repository": "https://git.sr.ht/~ledge/ng-app",
	"bugs": {
		"url": "https://github.com/zackschuster/ng-app/issues"
	},
	"license": "EUPL-1.2",
	"files": [
		"index.ts",
		"src",
		"build"
	],
	"main": "index.ts",
	"types": "index.ts",
	"type": "module",
	"scripts": {
		"start": "webpack serve",
		"prepare": "rollup -c rollup.config.ts",
		"prepublishOnly": "npm test",
		"docs": "webpack --env production",
		"lint": "tslint -p . -t msbuild --fix",
		"test": "ava"
	},
	"dependencies": {
		"@ledge/is-ie-11": "5.1.0",
		"@ledge/is-mobile": "6.1.0",
		"@ledge/types": "6.1.1",
		"@types/angular": "1.7.4",
		"@types/animejs": "3.1.2",
		"@types/lodash": "4.14.164",
		"@uirouter/angularjs": "1.0.19",
		"animejs": "3.2.1",
		"flatpickr": "4.6.6",
		"fuse.js": "6.4.3",
		"lodash": "4.17.20"
	},
	"peerDependencies": {
		"angular": "1.8.2",
		"angular-messages": "1.8.2",
		"bootstrap": "4.5.3",
		"font-awesome": "4.7.0"
	},
	"devDependencies": {
		"@ledge/configs": "23.0.32",
		"@rollup/plugin-commonjs": "16.0.0",
		"@rollup/plugin-node-resolve": "10.0.0",
		"@rollup/plugin-replace": "2.3.4",
		"@rollup/plugin-typescript": "6.1.0",
		"@types/html-webpack-plugin": "3.2.4",
		"abortcontroller-polyfill": "1.5.0",
		"angular-mocks": "1.8.2",
		"ava": "3.13.0",
		"browser-env": "3.3.0",
		"bundle-stats-webpack-plugin": "2.3.0",
		"copy-webpack-plugin": "6.3.0",
		"css-loader": "5.0.0",
		"file-loader": "6.2.0",
		"html-webpack-plugin": "4.5.0",
		"mini-css-extract-plugin": "1.2.1",
		"node-fetch": "2.6.1",
		"optimize-css-assets-webpack-plugin": "5.0.4",
		"pug": "3.0.0",
		"pug-loader": "2.4.0",
		"rollup": "2.33.1",
		"rollup-plugin-terser": "7.0.2",
		"sass": "1.28.0",
		"sass-loader": "10.0.5",
		"ts-loader": "8.0.7",
		"ts-node": "9.0.0",
		"tslint": "6.1.3",
		"tslint-loader": "3.5.4",
		"typescript": "4.0.5",
		"url-loader": "4.1.1",
		"webpack": "5.4.0",
		"webpack-cli": "4.1.0",
		"webpack-dev-server": "3.11.0",
		"whatwg-fetch": "3.4.1"
	},
	"ava": {
		"files": [
			"test/**",
			"!test/inputs/-util.ts",
			"!test/mocks/**"
		],
		"extensions": [
			"ts",
			"tsx"
		],
		"require": [
			"ts-node/register",
			"./test/-register.cjs"
		]
	}
}
