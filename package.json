{
	"name": "@ledge/ng-app",
	"version": "6.1.1",
	"description": "Powerful, easy-to-use app container for Angular.js with ES Modules. Types included. IE11 supported.",
	"homepage": "https://ng-app.js.org",
	"author": {
		"name": "Zack Schuster",
		"email": "email@zackschuster.com",
		"url": "http://so.completely.online"
	},
	"repository": "https://git.sr.ht/~ledge/ng-app",
	"bugs": {
		"url": "https://github.com/zackschuster/ng-app/issues"
	},
	"license": "EUPL-1.2",
	"files": [
		"index.ts",
		"src",
		"build"
	],
	"keywords": [
		"angular1",
		"angularjs",
		"angularjs1",
		"browser",
		"esm",
		"esmodules",
		"ie11",
		"modules",
		"ng-app",
		"ng1"
	],
	"type": "module",
	"types": "index.ts",
	"browser": "./build/ng-app.mjs",
	"module": "./build/ng-app.mjs",
	"main": "./build/ng-app.cjs",
	"exports": {
		"import": "./build/ng-app.mjs",
		"require": "./build/ng-app.cjs"
	},
	"scripts": {
		"lint": "tslint -p .",
		"predocs": "npm run prepare",
		"docs": "webpack --env production",
		"start": "webpack serve",
		"test": "ava",
		"prepare": "rollup -c rollup.config.ts",
		"prepublishOnly": "npm run lint && npm test -- --node-arguments='--title=../build/ng-app.cjs'"
	},
	"dependencies": {
		"@ledge/is-ie-11": "7.0.0",
		"@ledge/is-mobile": "7.0.0",
		"@ledge/jsx": "1.3.0",
		"@ledge/types": "6.1.1",
		"@types/angular": "1.8.0",
		"@uirouter/angularjs": "1.0.29"
	},
	"peerDependencies": {
		"angular": "^1.8.2",
		"angular-messages": "^1.8.2",
		"bootstrap": "^4.5.3"
	},
	"devDependencies": {
		"@babel/core": "7.12.10",
		"@babel/preset-env": "7.12.11",
		"@ledge/configs": "23.3.223",
		"@rollup/plugin-node-resolve": "11.1.0",
		"@rollup/plugin-replace": "2.3.4",
		"@rollup/plugin-typescript": "8.1.0",
		"@types/angular-mocks": "1.7.0",
		"angular-mocks": "1.8.2",
		"ava": "3.15.0",
		"babel-loader": "8.2.2",
		"browser-env": "3.3.0",
		"css-loader": "5.0.1",
		"file-loader": "6.2.0",
		"mini-css-extract-plugin": "1.3.4",
		"rollup": "2.36.1",
		"rollup-plugin-terser": "7.0.2",
		"sass": "1.32.4",
		"sass-loader": "10.1.1",
		"ts-loader": "8.0.14",
		"ts-node": "9.1.1",
		"tslint": "6.1.3",
		"tslint-loader": "3.5.4",
		"typescript": "4.1.3",
		"url-loader": "4.1.1",
		"webpack": "5.15.0",
		"webpack-cli": "4.3.1",
		"webpack-dev-server": "3.11.2"
	},
	"ava": {
		"files": [
			"test/**",
			"!test/inputs/-util.ts",
			"!test/mocks/**"
		],
		"extensions": [
			"ts",
			"tsx"
		],
		"require": [
			"browser-env/register",
			"ts-node/register",
			"angular/angular.js",
			"angular-mocks",
			"./test/-register.cjs"
		]
	}
}
