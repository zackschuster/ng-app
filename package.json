{
	"name": "@ledge/ng-app",
	"version": "6.0.3",
	"description": "Powerful, easy-to-use app container for Angular.js with ES Modules. Types included. IE11 supported.",
	"homepage": "https://ng-app.js.org",
	"author": {
		"name": "Zack Schuster",
		"email": "email@zackschuster.com",
		"url": "http://so.completely.online"
	},
	"repository": "https://git.sr.ht/~ledge/ng-app",
	"bugs": {
		"url": "https://github.com/zackschuster/ng-app/issues"
	},
	"license": "EUPL-1.2",
	"files": [
		"index.ts",
		"src",
		"build"
	],
	"type": "module",
	"types": "index.ts",
	"main": "./build/ng-app.cjs",
	"module": "./build/ng-app.mjs",
	"exports": {
		"import": "./build/ng-app.mjs",
		"require": "./build/ng-app.cjs"
	},
	"scripts": {
		"lint": "tslint -p . -t msbuild --fix",
		"docs": "webpack --env production",
		"start": "webpack serve",
		"test": "ava",
		"prepare": "rollup -c rollup.config.ts",
		"prepublishOnly": "npm run lint && npm test"
	},
	"dependencies": {
		"@ledge/is-ie-11": "7.0.0",
		"@ledge/is-mobile": "7.0.0",
		"@ledge/types": "6.1.1",
		"@types/angular": "1.8.0",
		"@uirouter/angularjs": "1.0.28"
	},
	"peerDependencies": {
		"angular": "^1.8.2",
		"angular-messages": "^1.8.2",
		"bootstrap": "^4.5.3"
	},
	"devDependencies": {
		"@ledge/configs": "23.0.32",
		"@rollup/plugin-node-resolve": "10.0.0",
		"@rollup/plugin-replace": "2.3.4",
		"@rollup/plugin-typescript": "6.1.0",
		"angular-mocks": "1.8.2",
		"ava": "3.13.0",
		"browser-env": "3.3.0",
		"copy-webpack-plugin": "6.3.0",
		"css-loader": "5.0.0",
		"file-loader": "6.2.0",
		"html-webpack-plugin": "4.5.0",
		"mini-css-extract-plugin": "1.2.1",
		"node-fetch": "2.6.1",
		"optimize-css-assets-webpack-plugin": "5.0.4",
		"pug": "3.0.0",
		"pug-loader": "2.4.0",
		"rollup": "2.33.1",
		"rollup-plugin-terser": "7.0.2",
		"sass": "1.28.0",
		"sass-loader": "10.0.5",
		"ts-loader": "8.0.8",
		"ts-node": "9.0.0",
		"tslint": "6.1.3",
		"tslint-loader": "3.5.4",
		"typescript": "4.0.5",
		"url-loader": "4.1.1",
		"webpack": "5.4.0",
		"webpack-cli": "4.1.0",
		"webpack-dev-server": "3.11.0"
	},
	"ava": {
		"files": [
			"test/**",
			"!test/inputs/-util.ts",
			"!test/mocks/**"
		],
		"extensions": [
			"ts",
			"tsx"
		],
		"require": [
			"ts-node/register",
			"./test/-register.cjs"
		]
	}
}
