	},
	"resolutions": {
		"chokidar": "3.4.3"
	},
	"ava": {
		"files": [
			"test/**/*.ts",
			"!test/inputs/-util.ts",
			"!test/mocks/**"
		],
		"extensions": [
			"ts"
		],
		"require": [
			"./test/-register.js"
		]
	}
